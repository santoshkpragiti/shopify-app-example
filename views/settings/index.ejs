<!DOCTYPE html>
<html>
<head>
  <script src="https://cdn.shopify.com/s/assets/external/app.js"></script>
  <script type="text/javascript">
    ShopifyApp.init({
      apiKey: '<%= apiKey %>',
      shopOrigin: 'https://<%= shop %>'
    });
    
  </script>
  <title>
    <%- token %>
  </title>
</head>
<body>
  <%- token %>
  <script type="text/javascript">
    ShopifyApp.ready(function() {
      ShopifyApp.Bar.initialize({
        title: '<%= token %>'
      });
    });
  </script>
</body>
</html>